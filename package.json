{
  "name": "spec-snake",
  "version": "0.0.1-beta.8",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/cut0/spec-snake.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/types.d.ts",
  "main": "./dist/types.js",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "default": "./dist/types.js"
    }
  },
  "bin": {
    "spec-snake": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:cli && pnpm build:client",
    "build:cli": "tsx scripts/build-cli.ts",
    "build:client": "vite build",
    "build:types": "tsc src/types.ts --declaration --outDir dist --skipLibCheck --module ESNext --moduleResolution bundler --target ES2022",
    "dev": "NODE_ENV=development SPEC_SNAKE_CONFIG=examples/local/spec-snake-ja.config.ts vite",
    "prd": "pnpm build && pnpm build:client && node dist/cli.js",
    "i18n": "lingui extract && lingui compile",
    "i18n:compile": "lingui compile",
    "i18n:extract": "lingui extract",
    "lint:check": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "0.1.75",
    "@hono/node-server": "1.19.7",
    "citty": "0.1.6",
    "consola": "3.4.2",
    "hono": "4.11.1",
    "jiti": "2.6.1",
    "valibot": "1.2.0"
  },
  "devDependencies": {
    "@base-ui/react": "1.0.0",
    "@biomejs/biome": "1.9.4",
    "@changesets/cli": "2.28.1",
    "@hono/vite-dev-server": "0.23.0",
    "@hookform/resolvers": "5.2.2",
    "@lingui/babel-plugin-lingui-macro": "5.7.0",
    "@lingui/cli": "5.7.0",
    "@lingui/conf": "5.7.0",
    "@lingui/core": "5.7.0",
    "@lingui/react": "5.7.0",
    "@lingui/vite-plugin": "5.7.0",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.35.1",
    "@tanstack/react-router": "1.141.6",
    "@tanstack/router-plugin": "1.141.7",
    "@types/node": "^25.0.3",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "esbuild": "0.27.2",
    "motion": "12.23.26",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.69.0",
    "react-markdown": "10.1.0",
    "remark-gfm": "4.0.1",
    "tailwindcss": "4.1.18",
    "tsx": "4.20.6",
    "typescript": "5.8.3",
    "vite": "7.3.0",
    "zustand": "5.0.9"
  },
  "packageManager": "pnpm@10.26.2"
}
